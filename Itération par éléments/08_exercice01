livres = [
    {"titre": "Python Facile", "auteur": "Jean Dupont", "annee": 2015},
    {"titre": "Algo 101", "auteur": "Claire Denis", "annee": 2009},
    {"titre": "IA moderne", "auteur": "Marc Zola", "annee": 2022}
]

print("Livres publiés après 2010 :")
for livre in livres:
    if livre["annee"] > 2010:
        print(f'{livre["titre"]} ({livre["annee"]})')
